{
  "name": "cloodle-kirby-dev",
  "version": "1.0.0",
  "description": "Kirby CMS development with Zero One theme patterns and UIkit integration for Cloodle platform",
  "author": "Cloodle Team",
  "contexts": ["kirby-core", "zero-one-theme", "uikit-patterns"],
  "keywords": ["kirby", "cms", "php", "uikit", "zero-one", "blueprints", "blocks"],
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  }
}
