{
  "name": "cloodle-content-native",
  "version": "1.0.0",
  "description": "Kirby to native Moodle content export (Book, Page, H5P) with consistent Cloodle styling",
  "author": "Cloodle Team",
  "contexts": ["moodle-content", "kirby-blocks", "h5p-integration"],
  "keywords": ["moodle", "kirby", "content", "book", "page", "h5p", "export"],
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      }
    }
  }
}
